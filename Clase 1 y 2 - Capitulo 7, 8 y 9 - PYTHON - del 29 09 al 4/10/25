        "# Importar las bibliotecas de visualizaci√≥n\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "\n",
        "# Seleccionar solo las columnas num√©ricas relevantes para la correlaci√≥n\n",
        "df_numerico = df_acciones[['close', 'open', 'high', 'low', 'volume']]\n",
        "\n",
        "# Calcular la matriz de correlaci√≥n entre las variables num√©ricas\n",
        "matriz_correlacion = df_numerico.corr()\n",
        "\n",
        "plt.figure(figsize=(8, 6))\n",
        "# 'annot=True' muestra los valores num√©ricos en las celdas\n",
        "# 'cmap='coolwarm'' usa un gradiente de colores que es f√°cil de interpretar\n",
        "sns.heatmap(matriz_correlacion, annot=True, cmap='coolwarm', fmt=\".2f\")\n",
