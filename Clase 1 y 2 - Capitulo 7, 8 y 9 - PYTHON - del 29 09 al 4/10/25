        "# Datos de ventas como un diccionario de Python\n",
        "ventas_data = {\n",
        "    'Producto': ['Camiseta', 'Pantal√≥n', 'Zapatos', 'Gorra', 'Sudadera'],\n",
        "    'Cantidad': [50, 30, 25, 45, 60],\n",
        "    'Precio_Unitario': [15.99, 39.50, 65.00, 12.25, 45.75]\n",
        "}\n",
        "\n",
        "# Crear el DataFrame\n",
        "df_ventas = pd.DataFrame(ventas_data)\n",
        "\n",
        "# Mostrar el DataFrame\n",
        "print(\"DataFrame inicial de ventas:\")\n",
        "print(df_ventas)\n",
        "\n",
        "# Explorar la estructura del DataFrame\n",
        "print(\"\\n---\")\n",
        "print(\"Dimensiones del DataFrame (filas, columnas):\", df_ventas.shape)\n",
