        "# Creamos una lista de diccionarios, donde cada diccionario representa un empleado\n",
        "empleados_data = [\n",
        "    {'ID': 101, 'Nombre': 'Elena', 'Departamento': 'Marketing'},\n",
        "    {'ID': 102, 'Nombre': 'Carlos', 'Departamento': 'Ventas'},\n",
        "    {'ID': 103, 'Nombre': 'Sof√≠a', 'Departamento': 'IT'},\n",
        "    {'ID': 104, 'Nombre': 'Jorge', 'Departamento': 'Marketing'},\n",
        "]\n",
        "\n",
        "# Convertimos la lista de diccionarios en un DataFrame\n",
        "df_empleados = pd.DataFrame(empleados_data)\n",
        "\n",
        "print(\"DataFrame creado a partir de una lista de diccionarios:\")\n",
        "print(df_empleados)"
      ],
